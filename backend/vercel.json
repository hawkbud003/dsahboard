{
    "version": 2,
    "builds": [
        {
            "src": "dsp/wsgi.py",
            "use": "@vercel/python"
        },
        {
            "src": "staticfiles/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "dsp/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "dsp.settings",
        "PYTHONPATH": ".",
        "VERCEL": "1"
    }
} 